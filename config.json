{
  "editor": {
    "windowWidth": 900,
    "windowHeight": 640,
    "fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "fontSize": "13px"
  },
  "yodakaPath": "src/Yodaka",
  "spagoCommand": "spago bundle-app -n",
  "spagoOptions": {
    "targetFile": "index.js",
    "mainPath": "src/Main",
    "bundlePath": "./Bundle.purs"
  },
  "osc": {
    "host": "127.0.0.1",
    "port": 5000
  },
  "timeout": 1000,
  "kusabiPort": 3000,
  "replSocketPort": 9000,
  "renderView": {
    "cameraRotation": false,
    "backgroundColor": "0x8844ff",
    "lightColor": "0xffffff",
    "ambientLightColor": "0xffffff",
    "postProcessing": true
  }
}
